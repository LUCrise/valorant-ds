<h1>詳細情報新規作成ページ</h1>

    <%= form_with(model:@post,url:agent_posts_path) do |f| %>
      <% if @post.errors.any? %>
        <div id="error_explanation" class = "alert alert-warning">
          <ul class="mb-0">
            <% @post.errors.full_messages.each do |msg| %>
              <li><%= msg %></li>
            <% end %>
          </ul>
        </div>
      <% end %>

      <div>
        <%= f.label :content, '追加する情報' %>
        <%= f.text_field :content %>
      </div>

      <%= f.submit '作成'%>
    <% end %>